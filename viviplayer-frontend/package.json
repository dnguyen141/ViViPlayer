{
  "name": "viviplayer-frontend",
  "private": true,
  "scripts": {
    "dev": "concurrently  \"next dev\" \"nodemon ./backend/index.js\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier \"pages/**/*.{js,css}\" \"styles/**/*.{js,css}\"",
    "format": "npm run prettier -- --write"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.16.13",
    "concurrently": "^6.4.0",
    "husky": "^7.0.4",
    "next": "12.0.4",
    "next-redux-wrapper": "^7.0.5",
    "prettier": "^2.4.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.1",
    "sass": "^1.43.4",
    "socket.io": "^4.4.0",
    "socket.io-client": "^4.4.0",
    "video.js": "^7.17.0",
    "videojs-markers": "^1.0.1",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "eslint": "7",
    "eslint-config-next": "12.0.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "prettier --write"
  }
}
